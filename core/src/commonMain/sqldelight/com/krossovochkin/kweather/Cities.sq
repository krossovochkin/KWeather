CREATE TABLE cities (
  id INTEGER NOT NULL,
  name TEXT NOT NULL
);

selectAll:
SELECT * FROM cities;

selectAllLimit:
SELECT * FROM cities LIMIT ?;

selectByQuery:
SELECT * FROM cities WHERE name LIKE ?;

selectByQueryLimit:
SELECT * FROM cities WHERE name LIKE ? LIMIT ?;

insert:
INSERT INTO cities(id, name) VALUES(?, ?);

deleteAll:
DELETE FROM cities;

beginTransaction:
BEGIN;

commitTransaction:
COMMIT;

rollbackTransaction:
ROLLBACK;